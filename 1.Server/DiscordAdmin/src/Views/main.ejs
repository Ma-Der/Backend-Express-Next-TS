<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/grid.css" >
    <link rel="stylesheet" href="/css/main.css" >
    <title>Discord Admin Dashboard</title>
</head>
<body>
    <header id="header">
        <div id="up-bar">
            <div id="user-account">
                <a href="/logout">Log Out</a>
            </div>
        </div>
        <div id="left-bar">
            <div class="sign-wrapper">
                <a href="/invite" target="_blank" rel="noopener noreferrer"><i class="fas fa-plus"></i></a>
            </div>
            <div id="choose-guild">
                <ul>
                <% commonGuilds.forEach((guild) => { %>
                        <li class="guild">
                            <a href="/dashboard/<%=guild.id%>/guild-menu"><%= guild.name %></a>
                        </li>
                <% }) %>
            </ul>
            </div>
        </div>
    </header>
    <main id="main" class="container">
        <%- include('welcome-main', {guilds: guilds}); %>

    </main>
    

    <footer id="footer"></footer>
    <script src="https://kit.fontawesome.com/19b46db47e.js" crossorigin="anonymous"></script>
</body>
</html>