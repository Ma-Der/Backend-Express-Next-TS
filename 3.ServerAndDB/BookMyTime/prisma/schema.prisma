generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Auth {
  id            Int    @id @default(autoincrement())
  userId        String @unique
  access_token  String
  refresh_token String
  scope         String
  token_type    String
  expiry_date   Int
}

model Event {
  id             Int      @id @default(autoincrement())
  summary        String
  location       String
  description    String
  eventStartTime DateTime
  startTimeZone  String
  eventEndTime   DateTime
  endTimeZone    String
}
